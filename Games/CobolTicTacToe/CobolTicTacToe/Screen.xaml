        <Button Content="New Game" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="btnNewGame" VerticalAlignment="Top" Width="75" Click="btnNewGame_Click" />
        <CheckBox Content="Player Starts First" Height="16" HorizontalAlignment="Left" Margin="93,16,0,0" Name="cbxPlayerStartsFirst" VerticalAlignment="Top" IsChecked="True" />
    </Grid>
</Window>
